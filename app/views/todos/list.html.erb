<div class="container">
	<div class="row">
		<div class="col-md-6">
			<h3>Complete</h3>
			<% @todos.each do |todo| %>
				<% if todo.completed === true %>
					<div class="well">
						<%= todo.description %>
					</div>
				<% end %>
			<% end %>
		</div>
		<div class="col-md-6">
			<h3>Incomplete</h3>
			<% @todos.each do |todo| %>
				<% if todo.completed === false %>
					<div class="well">
						<%= todo.description %>
					</div>
				<% end %>
			<% end %>
		</div>
	</div>
</div>
